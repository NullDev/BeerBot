{
  "name": "beerbot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Discord Bot for the BundesBeer Server",
  "author": "NullDev",
  "license": "MIT",
  "main": "src/app.js",
  "scripts": {
    "start:dev": "bun run lint && cross-env NODE_ENV=development bun src/app.js",
    "start:prod": "cross-env NODE_ENV=production bun src/app.js",
    "start": "bun run start:dev",
    "lint": "eslint . --ignore-pattern .venv",
    "lint:fix": "eslint . --fix",
    "generate-config": "bun scripts/generate-config.js",
    "export-db": "bun scripts/export-db.js"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/eslint-parser": "^7.27.5",
    "@types/bun": "latest",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "bun.db": "^1.1.2",
    "check-dependencies": "^2.0.0",
    "discord.js": "^14.21.0",
    "node-cron": "^4.2.0",
    "openai": "^5.8.2"
  }
}
